<%= form_with model: @event do |f| %>
  <div>
    <%= f.label :title %><br />
    <%= f.text_area :title, autofocus: true %>
  </div>

  <div>
  <%= f.label :location %><br />
  <%= f.text_area :location, autocomplete: "location" %>
  </div>

  <div>
    <%= f.label :date %><br />
    <%= f.date_field :date %>
  </div>

  <div>
    <%= f.button :submit %>
  </div>
<% end %>
